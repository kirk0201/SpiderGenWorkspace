<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 600px; background-color: rgb(239, 239, 239);" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ATabView&quot;,&quot;AButton&quot;]}" data-browser-width="600" data-browser-height="600">
    <div data-base="ATabView" data-class="ATabView" class="ATabView-Style" data-state="0" style="width: 100%; height: calc((100% - 60px) - 0px); left: 0px; top: 60px;" data-sgap-height="1" data-stretch-height="true" id="MainView--tabview">
        <div class="tab_area">
            <span class="ATabView_select">tab1</span>
            <span class="ATabView_deselect">tab2</span>
            <span class="ATabView_deselect">tab3</span>
        </div>
        <div class="tab_contents"></div>
    </div>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 80px; height: 22px; left: 10px; top: 10px;" id="MainView--backBtn" data-listener-click="MainView:onBackBtnClick">&lt;-</button>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 100px; height: 40px; top: 10px; right: 20px;" id="MainView--openBtn" data-listener-click="MainView:onOpenBtnClick">open window</button>
</div>