<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(239, 239, 239);" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ATextField&quot;,&quot;AButton&quot;]}" data-browser-width="470" data-browser-height="600">
    <input data-base="ATextField" data-class="ATextField" type="text" value="" class="ATextField-Style" autocomplete="off" style="width: calc((100% - 30px) - 80px); height: 40px; left: 30px; top: 30px; pointer-events: auto;" data-sgap-width="1" data-stretch-width="true" id="MainView--inputEdit" data-listener-focus="MainView:onInputEditFocus" data-timer-change="true" data-listener-change="MainView:onInputEditChange" placeholder="검색어 입력" data-listener-blur="MainView:onInputEditBlur" data-listener-keydown="MainView:onInputEditKeyDown">
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 60px; height: 40px; top: 30px; right: 10px;" id="MainView--searchBtn" data-listener-click="MainView:onSearchBtnClick">검색</button>
</div>