<div data-base="AView" data-class="LoginView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(239, 239, 239); position: absolute; font-size: 24px;" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;AButton&quot;,&quot;ATextField&quot;]}">
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: 150px; height: 70px; position: absolute; left: calc(50% - 75px); top: 420px;" data-listener-click="LoginView:onNextBtnClick" data-center-x="true">Login</button>
    <input data-base="ATextField" data-class="ATextField" type="text" value="" class="ATextField-Style" autocomplete="off" style="width: 380px; height: 70px; position: absolute; left: calc(50% - 184px); top: 150px;" data-center-x="true" placeholder="Click Login Button.">
    <input data-base="ATextField" data-class="ATextField" type="password" value="Text" class="ATextField-Style" autocomplete="off" style="width: 380px; height: 70px; position: absolute; left: calc(50% - 184px); top: 250px;" data-center-x="true">
