<div data-base="AView" data-class="MainView" class="AView-Style" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(255, 233, 10); position: absolute; min-width: 300px;" data-browser-width="500" data-browser-height="600" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;AImage&quot;,&quot;ATextField&quot;,&quot;ACheckBox&quot;,&quot;AButton&quot;,&quot;ALabel&quot;]}">
    <img data-base="AImage" data-class="AImage" class="AImage-Style" style="width: 130px; height: 120px; position: absolute; left: calc(50% - 65px); top: 100px;" src="Assets/Login/KakaoTalk_logo.png" data-center-x="true" id="MainView--kakaoImg">
    <input data-base="ATextField" data-class="ATextField" type="email" value="" class="ATextField-Style" autocomplete="off" style="width: calc((100% - 50px) - 50px); height: 50px; position: absolute; left: 50px; font-size: 17px; padding-left: 30px; padding-right: 30px; font-weight: normal; color: rgb(191, 191, 213); border-bottom-width: 1px; border-bottom-color: rgba(0, 0, 0, 0); letter-spacing: -2px; border-radius: 10px 10px 0px 0px; top: 300px; background-color: rgb(255, 255, 255);" data-sgap-width="1" placeholder="카카오계정 (이메일)" id="MainView--inputIdField" data-group="아이디(메일) 필드" data-listener-focus="MainView:onInputIdFieldFocus" data-listener-change="MainView:onInputIdFieldChange" data-listener-blur="MainView:onInputIdFieldBlur" data-listener-keydown="MainView:onInputIdFieldKeydown">
    <input data-base="ATextField" data-class="ATextField" type="password" value="" class="ATextField-Style" autocomplete="off" style="width: calc((100% - 50px) - 50px); height: 50px; position: absolute; left: 50px; font-size: 17px; padding-left: 30px; padding-right: 30px; color: rgb(191, 191, 213); letter-spacing: -2px; border-radius: 0px 0px 10px 10px; top: 350px; background-color: rgb(255, 255, 255);" data-sgap-width="1" placeholder="비밀번호" id="MainView--inputPassField" data-group="패스워드 필드">
    <span data-base="ACheckBox" data-class="ACheckBox" class="ACheckBox-Style" tabindex="0" style="width: auto; height: 20px; position: absolute; left: 50px; top: 430px; color: rgb(164, 151, 0); padding-left: 20px; padding-right: 0px; background-position: left center;" data-check-value="false" id="MainView--autoLoginCheck" data-group="자동로그인 체크박스">자동로그인</span>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: calc((100% - 50px) - 50px); height: 50px; position: absolute; left: 50px; top: 480px; border-radius: 10px; font-size: 17px; letter-spacing: -2px; color: rgb(255, 255, 255); background-color: rgb(113, 92, 94); cursor: pointer;" data-sgap-width="1" data-style-over=" loginBtnOver" id="MainView--loginBtn" data-group="로그인 버튼" data-listener-click="MainView:onLoginBtnClick">로그인</button>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: auto; height: auto; position: absolute; background-color: rgba(0, 0, 0, 0); left: 100px; bottom: 30px;" data-group="">카카오계정 찾기</button>
    <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: 1px; height: 17px; position: absolute; border-left: 1px solid rgb(191, 191, 213); left: 50%; bottom: 30px;"></label>
    <button data-base="AButton" data-class="AButton" class="AButton-Style" style="width: auto; height: auto; position: absolute; background-color: rgba(0, 0, 0, 0); right: 100px; bottom: 30px;">비밀번호 재설정</button>
</div>